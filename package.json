{
  "$schema": "https://www.schemastore.org/package.json",
  "author": "Bernd Matzner <bernd.matzner@regard.io>",
  "bugs": {
    "url": "https://github.com/regardio/js/issues"
  },
  "dependencies": {
    "intl-parse-accept-language": "1.0.0",
    "react-router": "7.11.0"
  },
  "description": "Regardio JavaScript utilities",
  "devDependencies": {
    "@regardio/dev": "1.9.4",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "25.0.3",
    "vitest": "4.0.16"
  },
  "exports": {
    "./async/delay": {
      "import": "./dist/async/delay.js",
      "types": "./dist/async/delay.d.ts"
    },
    "./browser/base64": {
      "import": "./dist/browser/base64.js",
      "types": "./dist/browser/base64.d.ts"
    },
    "./format/bytes": {
      "import": "./dist/format/bytes.js",
      "types": "./dist/format/bytes.d.ts"
    },
    "./format/measure": {
      "import": "./dist/format/measure.js",
      "types": "./dist/format/measure.d.ts"
    },
    "./http/cookie": {
      "import": "./dist/http/cookie.js",
      "types": "./dist/http/cookie.d.ts"
    },
    "./http/domain": {
      "import": "./dist/http/domain.js",
      "types": "./dist/http/domain.d.ts"
    },
    "./http/request-helpers": {
      "import": "./dist/http/request-helpers.js",
      "types": "./dist/http/request-helpers.d.ts"
    },
    "./intl/language-detector": {
      "import": "./dist/intl/language-detector.js",
      "types": "./dist/intl/language-detector.d.ts"
    },
    "./intl/locale": {
      "import": "./dist/intl/locale.js",
      "types": "./dist/intl/locale.d.ts"
    },
    "./time/time": {
      "import": "./dist/time/time.js",
      "types": "./dist/time/time.d.ts"
    },
    "./validation/invariant": {
      "import": "./dist/validation/invariant.js",
      "types": "./dist/validation/invariant.d.ts"
    },
    "./validation/verify-file-accept": {
      "import": "./dist/validation/verify-file-accept.js",
      "types": "./dist/validation/verify-file-accept.d.ts"
    }
  },
  "files": ["dist"],
  "homepage": "https://github.com/regardio/js#readme",
  "keywords": [
    "utilities",
    "javascript",
    "typescript",
    "http",
    "intl",
    "validation",
    "time",
    "format"
  ],
  "license": "MIT",
  "name": "@regardio/js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regardio/js"
  },
  "scripts": {
    "build": "exec-tsc --project tsconfig.build.json",
    "clean": "exec-clean .turbo dist",
    "dev": "exec-tsc --project tsconfig.build.json --watch",
    "fix": "exec-p fix:*",
    "fix:biome": "lint-biome check --write --unsafe .",
    "fix:md": "lint-md --fix",
    "lint": "exec-p lint:*",
    "lint:biome": "lint-biome check .",
    "lint:md": "lint-md",
    "prepare": "exec-husky",
    "release": "flow-release",
    "report": "exec-s report:*",
    "report:unit": "vitest run --reporter html",
    "test": "exec-s test:*",
    "test:unit": "vitest run",
    "typecheck": "exec-tsc --noEmit",
    "version": "flow-changeset version"
  },
  "sideEffects": false,
  "type": "module",
  "version": "0.2.3"
}
