{
  "$schema": "https://www.schemastore.org/package.json",
  "name": "@regardio/js",
  "version": "0.7.2",
  "private": false,
  "description": "Regardio JavaScript utilities",
  "keywords": [
    "utilities",
    "javascript",
    "typescript",
    "http",
    "intl",
    "validation",
    "time",
    "format"
  ],
  "homepage": "https://github.com/regardio/js#readme",
  "bugs": {
    "url": "https://github.com/regardio/js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regardio/js.git"
  },
  "license": "MIT",
  "author": "Bernd Matzner <bernd.matzner@regard.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./assert": {
      "types": "./dist/assert/index.d.mts",
      "import": "./dist/assert/index.mjs"
    },
    "./encoding": {
      "types": "./dist/encoding/index.d.mts",
      "import": "./dist/encoding/index.mjs"
    },
    "./http": {
      "types": "./dist/http/index.d.mts",
      "import": "./dist/http/index.mjs"
    },
    "./intl": {
      "types": "./dist/intl/index.d.mts",
      "import": "./dist/intl/index.mjs"
    },
    "./text": {
      "types": "./dist/text/index.d.mts",
      "import": "./dist/text/index.mjs"
    },
    "./time": {
      "types": "./dist/time/index.d.mts",
      "import": "./dist/time/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown && pnpm fix",
    "clean": "exec-clean .turbo dist",
    "dev": "tsdown --watch",
    "fix": "exec-p fix:*",
    "fix:biome": "lint-biome check --write --unsafe .",
    "fix:md": "lint-md --fix",
    "fix:pkg": "lint-package --fix",
    "lint": "exec-p lint:*",
    "lint:biome": "lint-biome check .",
    "lint:md": "lint-md",
    "lint:pkg": "lint-package",
    "prepare": "exec-husky",
    "release": "flow-release",
    "report": "vitest run --coverage",
    "test": "exec-s test:*",
    "test:unit": "vitest run",
    "typecheck": "exec-tsc --noEmit",
    "version": "flow-changeset version"
  },
  "dependencies": {
    "intl-parse-accept-language": "1.0.0",
    "react-router": "7.13.0"
  },
  "devDependencies": {
    "@regardio/dev": "1.13.3",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "25.2.2",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "tsdown": "0.20.3",
    "vitest": "4.0.18"
  },
  "publishConfig": {
    "access": "public"
  }
}
